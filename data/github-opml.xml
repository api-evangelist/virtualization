<opml version="1.0">
<head>
<title>API Monitoring Blog OPML</title>
</head>
<body>
  {% for org in site.data.organizations %}{% if org.github != '' %}<outline title="{{ org.name }}" text="{{ org.name }} Github" type="rss" version="RSS" xmlUrl="{{ org.github }}.atom" htmlUrl="{{ org.github }}"/>{% endif %}{% endfor %}
</body>
</opml>
